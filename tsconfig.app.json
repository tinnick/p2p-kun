/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src/",
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@models/*": ["app/models/*"],
      "@core/*": ["app/core/*"],
      "@pages/*": ["app/pages/*"],
      "@shared/*": ["app/shared/*"]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
